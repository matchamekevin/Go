{
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true,
    "target": "ES2021",
    "types": [
      "react",
      "react-native",
      "expo-router"
    ],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node16",
    "module": "Node16"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "extends": "expo/tsconfig.base"
}
