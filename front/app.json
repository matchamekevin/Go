{
  "expo": {
    "name": "GoSOTRAL",
    "slug": "gosotral",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "gosotral-front",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [["expo-router"]]
    ,
    "sdkVersion": "54.0.0"
  }
}
